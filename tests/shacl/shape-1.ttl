@prefix : <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix uq: <http://siriusâˆ’labs.no/shapes/unique#> .

:Emp a sh:NodeShape, rdfs:Class;
    sh:property [
        sh:path <http://example.org/Emp#E_id>;
        sh:nodeKind sh:Literal;
        sh:maxCount 1;
        sh:minCount 1;
        sh:datatype xsd:integer
    ];
    uq:uniqueValuesForClass [
        uq:unqProp <http://example.org/Emp#E_id>;
        uq:unqForClass :Emp
    ];
    sh:property [
        sh:path <http://example.org/Emp#Name>;
        sh:nodeKind sh:Literal;
        sh:maxCount 1;
        sh:minCount 1;
        sh:datatype xsd:string
    ];
    sh:property [
        sh:path <http://example.org/Emp#Post>;
        sh:nodeKind sh:Literal;
        sh:maxCount 1;
        sh:datatype xsd:string 
    ];
    sh:property [
        sh:path <http://example.org/Asg#ToEmp,ToPrj,E_id,P_id>;
        sh:nodeKind sh:IRI;
        sh:class :Prj
    ].

:Prj a sh:NodeShape, rdfs:Class;
    sh:property [
        sh:path <http://example.org/Prj#P_id>;
        sh:nodeKind sh:Literal;
        sh:maxCount 1;
        sh:minCount 1;
        sh:datatype xsd:integer
    ]; 
    uq:uniqueValuesForClass [ 
        uq:unqProp <http://example.org/Prj#P_id>;
        uq:unqForClass :Prj 
    ];
    sh:property [
        sh:path <http://example.org/Prj#Name>;
        sh:nodeKind sh:Literal;
        sh:maxCount 1;
        sh:minCount 1;
        sh:datatype xsd:string
    ];
    sh:property [
        sh:path <http://example.org/Prj#ToAcc>;
        sh:nodeKind sh:Literal;
        sh:maxCount 1;
        sh:minCount 1;
        sh:datatype xsd:integer
    ];
    uq:uniqueValuesForClass [
        uq:unqProp <http://example.org/Prj#ToAcc>;
        uq:unqForClass :Prj
    ];
    sh:property [
        sh:path [
            sh:inversePath <http://example.org/Asg#ToEmp,ToPrj,E_id,P_id>
        ];
        sh:nodeKind sh:IRI;
        sh:class :Emp
    ];
    sh:property [
        sh:path <http://example.org/Prj,Acc#ToAcc,A_id>;
        sh:nodeKind sh:IRI;
        sh:maxCount 1;
        sh:minCount 1;
        sh:class :Acc
    ].

:Acc a sh:NodeShape, rdfs:Class; 
    sh:property [
        sh:path <http://example.org/Acc#A_id>;
        sh:nodeKind sh:Literal;
        sh:maxCount 1;
        sh:minCount 1;
        sh:datatype xsd:integer
    ];
    uq:uniqueValuesForClass [
        uq:unqProp <http://example.org/Acc#A_id>;
        uq:unqForClass :Acc
    ];
    sh:property [
        sh:path <http://example.org/Acc#Name>;
        sh:nodeKind sh:Literal;
        sh:maxCount 1;
        sh:datatype xsd:string
    ];
    uq:uniqueValuesForClass [
        uq:unqProp <http://example.org/Acc#Name>;
        uq:unqForClass :Acc
    ];
    sh:property [
        sh:path [
            sh:inversePath <http://example.org/Prj,Acc#ToAcc,A_id>
        ];
        sh:nodeKind sh:IRI;
        sh:maxCount 1;
        sh:class :Prj
    ].